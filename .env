# .env.example
# Copy this file to .env and fill in your real values.
# NEVER commit your real .env file to public repos!

# OpenAI
OPENAI_API_KEY=sk-...

# Fastmail (see https://www.fastmail.com/help/technical/jmap/api.html)
FASTMAIL_USERNAME=your_fastmail_user
FASTMAIL_API_TOKEN=your_fastmail_app_password  # This is an app password, NOT your login password!
FASTMAIL_API_BASE_URL=https://api.fastmail.com/jmap
FASTMAIL_DOMAIN=fastmail.com

# Local LLM
USE_LOCAL_LLM=True
LOCAL_LLM_ENDPOINT=http://localhost:11434/api/generate

# Proxy
USE_TOR_PROXY=True
TOR_SOCKS_PROXY=socks5h://127.0.0.1:9050

# Misc
TIMEOUT=10
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 Chrome/113.0.0.0
